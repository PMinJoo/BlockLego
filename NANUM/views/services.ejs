		/* Open state */
		.menu-search .sb-search.sb-search-open,
		.menu-search .no-js .sb-search {
		    width: 100%;
		    -webkit-transition: width 0.8s;
		    -moz-transition: width 0.8s;
		    transition: width 0.8s;
		    -webkit-backface-visibility: hidden;
