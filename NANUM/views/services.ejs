		<div class="page-banner container-fluid no-padding">
			<div class="page-banner-content">
				<h3>Services</h3>
			</div>
		</div><!-- Page Banner /- -->

		<!-- Menu Section -->
		<div class="menu-section container-fluid no-padding">
			<br>
			<div class="container">
				<div class="section-header">
					<h3>Naum List</h3>
					<h5>Dongguk University</h5>
					<img src="/images/section-seprator.png" alt="section-seprator" width="169" height="15">
					<p>Please register the things you can share with your students.</p>
				</div><!-- Section Header /-  -->
					<% data.forEach(function (apply, index){ %>
			<form method="post" action="/apply<%- apply.nanum_id%>">
				<div class="menu-box">
					<h3><%= apply.title %><span>Up to <%= apply.quantity %> people</span></h3>
					<p><%= apply.kind %> | <%= apply.place %> | <%= apply.nanum_date %><br>
					<%= apply.write_date %> | <%= apply.writer %></p>
					<button type="button" href="/apply/regist" class="btn btn-outline-danger">Apply Now</button>
				</div>
			</form>
			<% })%>

			<center>
			<a class="btn btn-danger btn-lg" href="/write.html">Nanum Start</a></center>
			<br><br><br><br><br><p></p>
			</div><!-- Container /- -->
		</div><!-- Menu Section /- -->
